    "test": "test"
  },
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "light-server": "^1.1.5",
    "npm-run-all": "^1.5.1"
  },
  "devDependencies": {
    "d3": "^3.5.15"
    },
  "scripts": {
    "start": "npm-run-all --silent --parallel compile server open",
